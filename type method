class student:
    scool= "Telusku"
    def __init__(self,m1,m2,m3):
        self.m1=m1
        self.m2=m2
        self.m3=m3

    def avg(self):
        return(self.m1+self.m2+self.m3)/3
    @classmethod
    def get_schol(cls):
        return cls.scool
    @staticmethod
    def info():
        print("this is student class")
s1= student(34,36,32)
s2=student(20,40,60)
print(s1.avg())
print(student.get_schol())
student.info()
